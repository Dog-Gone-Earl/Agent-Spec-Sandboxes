            "show_title":true,
            "type":"group",
            "layout_type":"ordered",
            "widgets":[
               {
                  "id":5656856687363736,
                  "definition":{
                     "title":"Certificate Status",
                     "title_size":"16",
                     "title_align":"left",
                     "type":"check_status",
                     "check":"http.ssl_cert",
                     "grouping":"cluster",
                     "group":"host:ubuntu,instance:terry_ca_service,url:https://ubuntu:8080",
                     "group_by":[
                        "url"
                     ],
                     "tags":[
                        
                     ]
                  },
                  "layout":{
                     "x":0,
                     "y":0,
                     "width":2,
                     "height":2
                  }
               },
               {
                  "id":3935405060421064,
                  "definition":{
                     "title":"Connections",
                     "title_size":"16",
                     "title_align":"left",
                     "type":"query_table",
                     "requests":[
                        {
                           "queries":[
                              {
                                 "data_source":"metrics",
                                 "name":"query1",
                                 "query":"max:network.http.can_connect{*} by {host,url}",
                                 "aggregator":"max"
                              },
                              {
                                 "data_source":"metrics",
                                 "name":"query2",
                                 "query":"max:network.http.cant_connect{*} by {host,url}",
                                 "aggregator":"max"
                              }
                           ],
                           "response_format":"scalar",
                           "formulas":[
                              {
                                 "cell_display_mode":"bar",
                                 "alias":"Can Connect",
                                 "formula":"cutoff_min(query1, 1)",
                                 "limit":{
                                    "count":500,
                                    "order":"desc"
                                 }
                              },
                              {
                                 "cell_display_mode":"bar",
                                 "alias":"Can't Connect",
                                 "formula":"cutoff_min(query2, 1)"
                              }
                           ]
                        }
                     ],
                     "has_search_bar":"auto"
                  },
                  "layout":{
                     "x":2,
                     "y":0,
                     "width":5,
                     "height":3
                  }
               },
               {
                  "id":3229229152582186,
                  "definition":{
