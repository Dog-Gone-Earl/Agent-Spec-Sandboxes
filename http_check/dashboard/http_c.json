            "widgets":[
               {
                  "id":5957457956443450,
                  "definition":{
                     "type":"note",
                     "content":"### Identify soon-to-expire SSL certificates\n  - The Agent checks for soon-to-expire SSL certificates by default with the setting `check_certificate_expiration` .",
                     "background_color":"green",
                     "font_size":"14",
                     "text_align":"left",
                     "vertical_align":"center",
                     "show_tick":false,
                     "tick_pos":"50%",
                     "tick_edge":"bottom",
                     "has_padding":true
                  },
                  "layout":{
                     "x":0,
                     "y":0,
                     "width":12,
                     "height":1
                  }
               },
               {
                  "id":2060232188867920,
                  "definition":{
                     "title":"SSL Certificate Expiration",
                     "title_size":"16",
                     "title_align":"left",
                     "type":"query_table",
                     "requests":[
                        {
                           "response_format":"scalar",
                           "queries":[
                              {
                                 "name":"query1",
                                 "data_source":"metrics",
                                 "query":"max:http.ssl.days_left{*} by {url}",
                                 "aggregator":"max"
                              },
                              {
                                 "name":"query2",
                                 "data_source":"metrics",
                                 "query":"max:http.ssl.seconds_left{*} by {url}",
                                 "aggregator":"max"
                              }
                           ],
                           "formulas":[
                              {
                                 "cell_display_mode":"bar",
                                 "alias":"Years",
                                 "style":{
                                    "palette":"orange"
                                 },
                                 "formula":"query1 / 365"
                              },
                              {
                                 "alias":"Months",
                                 "cell_display_mode":"bar",
                                 "style":{
                                    "palette":"orange"
                                 },
                                 "formula":"query1 / 12"
                              },
                              {
                                 "alias":"Weeks",
                                 "cell_display_mode":"bar",
                                 "style":{
                                    "palette":"orange"
                                 },
                                 "formula":"query1 / 7"
                              },
                              {
                                 "alias":"Days",
                                 "cell_display_mode":"number",
                                 "style":{
                                    "palette":"orange"
                                 },
                                 "formula":"query1 * 1",
                                 "conditional_formats":[
                                    {
                                       "comparator":">",
                                       "value":7,
                                       "palette":"white_on_green"
                                    },
                                    {
                                       "comparator":"<",
                                       "value":7,
                                       "palette":"white_on_yellow"
                                    },
                                    {
                                       "comparator":"<",
                                       "value":3,
                                       "palette":"white_on_red"
                                    }
                                 ]
                              },
                              {
                                 "alias":"Hours",
                                 "cell_display_mode":"number",
                                 "style":{
                                    "palette":"orange"
                                 },
                                 "formula":"query1 * 24",
                                 "conditional_formats":[
                                    {
                                       "comparator":">",
                                       "value":24,
                                       "palette":"white_on_green"
                                    },
                                    {
                                       "comparator":"<",
                                       "value":24,
                                       "palette":"white_on_yellow"
                                    },
                                    {
                                       "comparator":"<",
                                       "value":4,
                                       "palette":"white_on_red"
                                    }
                                 ]
                              },
                              {
                                 "alias":"Minutes",
                                 "style":{
                                    "palette":"orange"
                                 },
                                 "formula":"query1 * 1440",
                                 "conditional_formats":[
                                    {
                                       "comparator":">",
                                       "value":60,
                                       "palette":"white_on_green"
                                    },
                                    {
                                       "comparator":"<",
                                       "value":60,
                                       "palette":"white_on_yellow"
                                    },
                                    {
                                       "comparator":"<",
                                       "value":30,
                                       "palette":"white_on_red"
                                    }
                                 ]
                              },
                              {
                                 "alias":"Seconds",
                                 "style":{
                                    "palette":"orange"
                                 },
                                 "formula":"query2 * 1"
                              }
                           ]
                        }
                     ],
                     "has_search_bar":"auto"
                  },
                  "layout":{
                     "x":0,
                     "y":1,
                     "width":12,
                     "height":4
                  }
               },
               {
                  "id":5483750549883794,
